        android:supportsRtl="true"



        >

        <activity android:name=".SplashscreenAndroTennis">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>




            </intent-filter>

        </activity>
        <activity android:name=".HomeActivity" android:screenOrientation="landscape">

        </activity>
        <activity android:name=".PerformActivity" android:screenOrientation="landscape"></activity>
        <activity android:name=".HistoricActivity" android:screenOrientation="landscape"></activity>
        <activity android:name=".AboutActivity" android:screenOrientation="landscape"></activity>
        <activity android:name=".AddPerformActivity" android:screenOrientation="landscape"></activity>

    </application>


</manifest>
