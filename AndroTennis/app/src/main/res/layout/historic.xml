<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TableLayout
        android:id="@+id/tablehisto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content">

            <TextView
                android:id="@+id/date"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Date"
                android:textAlignment="center"/>

            <TextView
                android:id="@+id/noma"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Nom"
                android:textAlignment="center"/>

            <TextView
                android:id="@+id/set1j"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Set1"
                android:textAlignment="center"/>

            <TextView
                android:id="@+id/set1a"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Set1 Adv."
                android:textAlignment="center"/>

            <TextView
                android:id="@+id/set2j"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Set2"
                android:textAlignment="center"/>

            <TextView
                android:id="@+id/set2a"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Set2 Adv."
                android:textAlignment="center"/>

            <TextView
                android:id="@+id/set3j"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Set3"
                android:textAlignment="center"/>

            <TextView
                android:id="@+id/set3a"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Set3 Adv."
                android:textAlignment="center"/>

            <TextView
                android:id="@+id/lieu"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Lieu"
                android:textAlignment="center"/>

        </TableRow>

    </TableLayout>
</LinearLayout>